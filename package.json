{
  "name": "sockets",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier-eslint --write src/**/*.ts",
    "precommit": "lint-staged",
    "start": "ts-node-dev --respawn src/server/app.ts"
